#include<iostream>
using namespace std;
int linearSearch(char str[],char ch)
{
    for(int i=0;str[i]>0;i++)
    {
        if(str[i]==ch)
        {
            cout<<"Find the First Occurrence of a Character in a String:"<<i<<" ";
            break;
        }
    }
    return -1;
}
int main()
{
    char str[] = "engineering";
    char ch = 'e';
    linearSearch(str,ch);
    return 0;
}
