#include<iostream>
using namespace std;
void greaterThan(int arr[],int size)
{
    for(int i=0;i<size;i++)
    {
        if(arr[i]>20)
        {   
            cout<<arr[i]<<" ";
        }
    }
}
int main()
{
    int arr[]={10,15,20,25,30};
    int size=sizeof(arr)/sizeof(arr[0]);
    greaterThan(arr,size);
}
